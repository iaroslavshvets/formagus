{
  "name": "formagus",
  "version": "2.0.41",
  "license": "MIT",
  "sideEffects": false,
  "authors": [
    {
      "name": "Iaroslav Shvets",
      "email": "iaroslavs@wix.com"
    }
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rollup --config",
    "test": "jest",
    "test:unit": "jest",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:ci-coverage": "jest --ci --coverage",
    "posttest": "yarn test:coverage",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "prepush": "yarn test",
    "lint": "yarn es-lint && yarn format",
    "format": "prettier --write '{src,__tests__}/**/*.{ts,tsx,js,json,scss,css}'",
    "es-lint": "eslint --fix '{src,__tests__}/**/*.{ts,tsx}'",
    "commit-cz": "git-cz"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "/dist/src",
    "/dist/index.cjs",
    "/dist/index.js"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@swc/core": "^1.3.64",
    "@swc/jest": "^0.2.26",
    "@testing-library/react": "^14.0.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.25.1",
    "rollup-plugin-size": "^0.3.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "trier-promise": "^1.0.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "mobx": "*",
    "mobx-react": "*",
    "react": "*",
    "react-dom": "*"
  },
  "config": {
    "commitizen": {
      "path": "./packages/lib/node_modules/cz-conventional-changelog"
    }
  }
}

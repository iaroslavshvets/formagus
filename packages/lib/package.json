{
  "name": "formagus",
  "version": "1.19.0",
  "license": "MIT",
  "sideEffects": false,
  "private": false,
  "authors": [
    {
      "name": "Iaroslav Shvets",
      "email": "iaroslavs@wix.com"
    }
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "test": "jest",
    "test:unit": "jest",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:ci-coverage": "jest --ci --coverage",
    "posttest": "npm run test:coverage",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "prepush": "npm run test",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx,js,json,scss,css}'",
    "commit-cz": "git-cz"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,scss,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix"
    ]
  },
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "/dist/src",
    "/dist/index.js"
  ],
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/flat": "^5.0.2",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "jest-cli": "^27.4.3",
    "lint-staged": "^12.1.2",
    "mobx": "^6.0.0",
    "mobx-react": "^7.0.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "trier-promise": "^1.0.1",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-wix": "^1.0.31",
    "typescript": "^4.5.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "flat": "^4.1.0",
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "mobx": "*",
    "mobx-react": "*",
    "react": "*",
    "react-dom": "*"
  },
  "config": {
    "commitizen": {
      "path": "./packages/lib/node_modules/cz-conventional-changelog"
    }
  }
}

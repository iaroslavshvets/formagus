{
  "name": "formagus",
  "version": "1.35.7-8",
  "license": "MIT",
  "sideEffects": false,
  "authors": [
    {
      "name": "Iaroslav Shvets",
      "email": "iaroslavs@wix.com"
    }
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rollup --config",
    "test": "jest",
    "test:unit": "jest",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:ci-coverage": "jest --ci --coverage",
    "posttest": "yarn test:coverage",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "prepush": "yarn test",
    "lint": "yarn es-lint && yarn format",
    "format": "prettier --write '{src,__tests__}/**/*.{ts,tsx,js,json,scss,css}'",
    "es-lint": "eslint --fix '{src,__tests__}/**/*.{ts,tsx}'",
    "commit-cz": "git-cz"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,scss,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "/dist/src",
    "/dist/index.cjs",
    "/dist/index.js"
  ],
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@testing-library/react": "^12.1.5",
    "@types/flat": "^5.0.2",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.192",
    "@types/react": "^17.0.57",
    "@types/react-dom": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "latest",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "lint-staged": "^13.2.1",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "prettier": "^2.8.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^3.20.2",
    "trier-promise": "^1.0.1",
    "ts-jest": "^27.1.5",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "prettier-eslint": "^15.0.1",
    "ts-loader": "^9.4.2"
  },
  "dependencies": {
    "flat": "^4.1.1",
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "mobx": "*",
    "mobx-react": "*",
    "react": "*",
    "react-dom": "*"
  },
  "config": {
    "commitizen": {
      "path": "./packages/lib/node_modules/cz-conventional-changelog"
    }
  }
}

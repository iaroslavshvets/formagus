{
  "name": "formagus",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "private": false,
  "authors": [
    {
      "name": "Iaroslav Shvets",
      "email": "iaroslavs@wix.com"
    }
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "test": "jest",
    "test:unit": "jest",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:ci-coverage": "jest --ci --coverage",
    "posttest": "npm run test:coverage",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "prepush": "npm run test",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx,js,json,scss,css}'",
    "commit-cz": "git-cz"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,scss,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix"
    ]
  },
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "/dist/src",
    "/dist/index.js"
  ],
  "devDependencies": {
    "@testing-library/react": "^10.4.8",
    "@types/flat": "^5.0.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "jest-cli": "^26.3.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.11",
    "mobx": "^5.15.5",
    "mobx-react": "^6.2.5",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "trier-promise": "^1.0.1",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-wix": "^1.0.31",
    "typescript": "^3.9.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "flat": "^4.1.0",
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "^4.17.19"
  },
  "peerDependencies": {
    "mobx": "*",
    "mobx-react": "*",
    "react": "*",
    "react-dom": "*"
  },
  "config": {
    "commitizen": {
      "path": "./packages/lib/node_modules/cz-conventional-changelog"
    }
  }
}

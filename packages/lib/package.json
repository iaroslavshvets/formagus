{
  "name": "formagus",
  "version": "0.0.22",
  "license": "MIT",
  "private": false,
  "authors": [
    {
      "name": "Iaroslav Shvets",
      "email": "iaroslavs@wix.com"
    }
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "test": "jest",
    "test-unit": "jest",
    "test-watch": "jest --watch --detectOpenHandles",
    "test-coverage": "jest --coverage",
    "posttest": "npm run test-coverage",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "prepush": "npm run test",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx,js,json,scss,css}'",
    "commit-cz": "git-cz"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,scss,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix"
    ]
  },
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "/dist/src",
    "/dist/index.js"
  ],
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/flat": "^0.0.28",
    "@types/jest": "^23.3.9",
    "@types/lodash-es": "^4.17.1",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.7",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "husky": "^1.1.2",
    "jest": "^23.5.0",
    "jest-cli": "^23.6.0",
    "jest-watch-typeahead": "^0.2.0",
    "lint-staged": "^8.0.4",
    "mobx": "^5.5.2",
    "mobx-react": "^5.2.5",
    "prettier": "^1.15.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-test-renderer": "^16.6.1",
    "trier-promise": "^1.0.1",
    "ts-jest": "^23.1.3",
    "ts-loader": "^5.3.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-config-wix": "^1.0.31",
    "typescript": "^3.1.4",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "flat": "^4.1.0",
    "lodash": "^4.17.8"
  },
  "peerDependencies": {
    "mobx": "*",
    "mobx-react": "*",
    "react": "*",
    "react-dom": "*"
  },
  "config": {
    "commitizen": {
      "path": "./packages/lib/node_modules/cz-conventional-changelog"
    }
  }
}
